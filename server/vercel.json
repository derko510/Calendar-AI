{
  "version": 2,
  "name": "calendar-ai-backend",
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true
    }
  },
  "env": {
    "NODE_ENV": "production",
    "LLM_PROVIDER": "groq",
    "LLM_MODEL": "llama-3.1-8b-instant",
    "PORT": "3001"
  }
}
